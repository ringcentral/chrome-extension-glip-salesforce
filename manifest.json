{
  "name": "RingCentral Team Messaging Salesforce",
  "description": "Link Salesforce cases to RingCentral Team Messaging teams",
  "version": "4.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://*.salesforce.com/5*",
        "https://*.lightning.force.com/lightning/*"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}

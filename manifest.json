{
  "name": "Glip Salesforce",
  "description" : "Link Salesforce cases to Glip teams",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.salesforce.com/5*"],
      "js": ["content.js"]
    }
  ]
}
